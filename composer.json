{
    "name": "ouestcode/ploi-cli",
    "description": "ploi.io CLI tool",
    "keywords": ["ploi.io", "ploi", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "William",
            "email": "work@suppo.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.0",
        "laravel-zero/framework": "^9.1.3",
        "nunomaduro/termwind": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["ploi"]
}
